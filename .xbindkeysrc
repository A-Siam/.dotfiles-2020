# System config
#
"rofi -show emoji -modi emoji"
    Mod4 + e

"$HOME/.scripts/rofi-alttab"
    Alt + Tab

"drawio"
    Mod4 + Shift + d

"inkscape"
    Mod4 + i

"magnus"
    Mod4 + Shift + m

"blueman-manager"
    Mod4 + b

"~/.scripts/dmenu-scripts"
    Mod4 + Shift + s

"$TERMINAL"
    Mod4 + Return

"$TERMINAL -e "vim $HOME/.scripts/todos.sh""
    Mod4 + Shift + a

"blueman-manager"
    Mod4 + b

"brightnessctl set 15-"
    Mod2 + XF86MonBrightnessDown

"brightnessctl set 15+"
    Mod2 + XF86MonBrightnessUp

"$BROWSER"
    Mod4 + w

"pamixer -i 5 --allow-boost"
    Mod2 +XF86AudioRaiseVolume

"pamixer -d 5 --allow-boost"
    Mod2 + XF86AudioLowerVolume
"pamixer -t"
Mod2 + XF86AudioMute

"$HOME/.scripts/res-xbindkeys"
    Mod4 + Mod2 + R

