// Place your key bindings in this file to override the defaultsauto[
[
    {
        "key": "ctrl+j ctrl+h",
        "command": "workbench.action.toggleActivityBarVisibility"
    },
    {
        "key": "ctrl+shift+v",
        "command": "editor.action.clipboardPasteAction"
    },
    {
        "key": "ctrl+v",
        "command": "-editor.action.clipboardPasteAction"
    },
    {
        "key": "ctrl+shift+v",
        "command": "-editor.action.clipboardPasteAction",
        "when": "vim.mode == 'Normal'"
    }    
]